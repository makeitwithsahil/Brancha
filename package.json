{
  "name": "brancha-site",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Brancha builds and maintains consistent online presence systems for service businesses across India. Professional websites, brand identity, and ongoing digital support.",
  "keywords": [
    "business website development",
    "digital transformation",
    "gym website",
    "healthcare website",
    "real estate website",
    "education management system",
    "lead generation",
    "WhatsApp automation",
    "service business India"
  ],
  "author": "Brancha",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && npm run generate-rss && npm run generate-sitemap",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "generate-rss": "node scripts/generateRSS.js",
    "generate-sitemap": "node scripts/generateSitemap.js",
    "generate-blog": "node scripts/generateBlogPost.js && npm run generate-rss",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit",
    "analyze": "vite build --mode analyze",
    "build:prod": "npm run clean && vite build && npm run generate-rss && npm run generate-sitemap"
  },
  "dependencies": {
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^4.1.18",
    "terser": "^5.37.0",
    "vite": "^5.2.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
